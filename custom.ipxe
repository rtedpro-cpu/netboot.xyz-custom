#!ipxe
###
### made by rtedpro-cpu
### collaborator Efebey007
###

:custom
clear choice
menu WindowsNetboot
item --gap Windows Installations
item option_one ${space} Boot into WindowsNetboot PE
item --gap Windows Recovery PE(s)
item option_two ${space} HBCD PE
choose choice || goto exit
echo ${cls}
goto ${choice}
goto exit

:option_one
insert the script for booting here ig
boot || goto exit

:option_two
imgfree
kernel http://boot.netboot.xyz/wimboot
initrd -n bootmgr     https://github.com/rtedpro-cpu/windowsnetboot/raw/refs/heads/main/release/custom/hbcd/bootmgr       bootmgr ||
initrd -n bootmgr.efi https://github.com/rtedpro-cpu/windowsnetboot/raw/refs/heads/main/release/custom/hbcd/bootmgr.efi   bootmgr.efi ||      
initrd -n bcd         https://github.com/rtedpro-cpu/windowsnetboot/raw/refs/heads/main/release/custom/hbcd/Boot/BCD      bcd ||
initrd -n boot.sdi    https://github.com/rtedpro-cpu/windowsnetboot/raw/refs/heads/main/release/custom/hbcd/Boot/boot.sdi boot.sdi ||
initrd -n boot.wim    https://github.com/rtedpro-cpu/windowsnetboot/releases/download/hbcd/boot.wim boot.wim
boot || goto exit
:exit
chain utils.ipxe
exit
